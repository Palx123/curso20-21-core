def cambio(cadena):
    total_1 = []
    total_2 = []
    total_3 = []
    for i in cadena:
        total_1.append(ord(i))
    for i in cadena:
        total_2.append(int(str(ord(i)).replace(1,7)))
    print(total_1)
    print(total_2)
    for i in total_1:
        for x in range(0,len(cadena -1)):
        total_3.append(total_2[i]- total_1[i])
    print(total_3)
            
    

cadena = ["1","2","3"]
